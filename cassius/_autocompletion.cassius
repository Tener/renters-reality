.ui-autocomplete-loading
  background: white url(#{staticLink "images/ui-anim_basic_16x16.gif"}) right center no-repeat

.ui-autocomplete
  width:      60px
  background: white
  border:     solid 1px black
  border-top: none
  padding: 0px
  margin:  0px

.ui-autocomplete li
  list-style: none
  padding:    2px 5px

.ui-autocomplete li:hover
  color:            blue
  background-color: #dedbd1
  cursor:           pointer
